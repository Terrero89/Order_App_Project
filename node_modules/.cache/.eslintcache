button className={classes[\"button--alt\"]} onClick={props.onClose}>\n        Close\n      </button>\n      {hasItems && (\n        <button className={classes.button} onClick={orderHandler}>\n          Order\n        </button>\n      )}\n    </div>\n  );\n\n  const cartModalContent = (\n    <React.Fragment>\n      {cartItems}\n      <div className={classes.total}>\n        <span>Total Amount</span>\n        <span>{totalAmount}</span>\n      </div>\n      {isCheckout && (\n        <Checkout onConfirm={submitOrderHandler} onCancel={props.onClose} />\n      )}\n      {!isCheckout && modalActions}\n    </React.Fragment>\n  );\n\n  const isSubmittingModalContent = <p>Sending order data...</p>;\n  const didSubmitModalContent = (\n    <React.Fragment>\n      <p>SUCCESSS!!</p>\n      <div className={classes.actions}>\n        <button className={classes[\"button--alt\"]} onClick={props.onClose}>\n          Close\n        </button>\n        \n     \n    \n      </div>\n    </React.Fragment>\n  );\n  return (\n    <Modal onClose={props.onClose}>\n      {!isSubmitting && !didSubmit && cartModalContent}\n      {isSubmitting && isSubmittingModalContent}\n      {!isSubmitting && didSubmit && didSubmitModalContent}\n    </Modal>\n  );\n};\nexport default Cart;\n","/home/terrero/Documents/Web Development/Frontend/React_Projects/food_order_app/src/components/UI/Modal.js",[],[],"/home/terrero/Documents/Web Development/Frontend/React_Projects/food_order_app/src/store/CartProvider.js",[],[],"/home/terrero/Documents/Web Development/Frontend/React_Projects/food_order_app/src/store/cart-context.js",[],[],"/home/terrero/Documents/Web Development/Frontend/React_Projects/food_order_app/src/components/Cart/CartItem.js",[],[],"/home/terrero/Documents/Web Development/Frontend/React_Projects/food_order_app/src/components/Cart/checkout.js",[],[],{"ruleId":"100","severity":1,"message":"101","line":31,"column":11,"nodeType":"102","messageId":"103","endLine":31,"endColumn":19},"no-unused-vars","'resp